{
  "private": true,
  "name": "remix-prisma-starter",
  "description": "A Remix starter with Prisma and Vercel",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .eslintcache playwright-report build public/build .cache node_modules",
    "start": "remix-serve build",
    "build": "remix build",
    "dev": "remix dev",
    "postinstall": "remix setup node",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "test": "dotenv -e .env.test -- bash -c 'prisma db push -- --force && npm run build && playwright test'",
    "test:ci": "prisma db push && npm run build && playwright test",
    "prepare": "husky install",
    "deploy": "npm run ts-run tasks/validateConfig.ts && prisma generate && prisma db push && npm run build",
    "ts-check": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "ts-run": "dotenv ts-node",
    "validate-env": "npm run ts-run tasks/validateConfig.ts",
    "ci": "npm run lint && npm run ts-check && npx playwright install && npm run test"
  },
  "dependencies": {
    "@prisma/client": "5.1.1",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@remix-run/vercel": "1.19.3",
    "@vercel/node": "^2.15.10",
    "@vercel/remix": "^1.19.2",
    "accept-language-parser": "^1.5.0",
    "argon2": "^0.31.0",
    "classnames": "^2.3.2",
    "daisyui": "^3.5.1",
    "date-fns": "^2.30.0",
    "isbot": "^3.6.13",
    "lodash": "^4.17.21",
    "postcss": "^8.4.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "remix": "1.19.3",
    "zod": "^3.21.4",
    "zod-error": "^1.5.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright-testing-library/test": "^4.5.0",
    "@playwright/test": "^1.37.0",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@types/accept-language-parser": "^1.5.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.0.0",
    "husky": ">=8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.1",
    "prisma": "5.1.1",
    "ts-node": "^10.9.1",
    "type-fest": "^4.1.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=v18.17.1"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix"
  }
}
