{
  "private": true,
  "name": "remix-prisma-starter",
  "description": "A Remix starter with Prisma and Vercel",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup node",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy && yarn db:generate",
    "db:gen:migration": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "lint": "yarn eslint \"**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@prisma/client": "3.8.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "accept-language-parser": "^1.5.0",
    "classnames": "^2.3.1",
    "daisyui": "^1.25.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.3.2",
    "remix": "^1.1.3",
    "tailwindcss": "^3.0.15",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/accept-language-parser": "^1.5.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "dotenv": "^14.2.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.4",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prisma": "3.8.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
