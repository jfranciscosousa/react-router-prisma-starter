{
  "private": true,
  "name": "remix-prisma-starter",
  "description": "A Remix starter with Prisma and Vercel",
  "license": "MIT",
  "scripts": {
    "start": "remix-serve build",
    "build": "NODE_ENV=production npm run build:css && remix build",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev": "NODE_ENV=development concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup node",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy && yarn db:generate",
    "db:gen:migration": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "lint": "yarn eslint \"**/*.{js,jsx,ts,tsx}\"",
    "test": ". ./.envrc.test && yarn db:reset --force && yarn db:migrate && yarn build && playwright test",
    "test:ci": "yarn db:migrate && playwright test",
    "ts-check": "tsc --project tsconfig.json --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "@remix-run/react": "^1.4.1",
    "@remix-run/serve": "^1.4.1",
    "@remix-run/vercel": "^1.4.1",
    "accept-language-parser": "^1.5.0",
    "argon2": "^0.28.5",
    "classnames": "^2.3.1",
    "daisyui": "^2.14.1",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-use": "^17.3.2",
    "remix": "^1.4.1",
    "tailwindcss": "^3.0.24",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.1",
    "@remix-run/dev": "^1.4.1",
    "@types/accept-language-parser": "^1.5.3",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "autoprefixer": "^10.4.5",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "prisma": "3.12.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
