{
  "private": true,
  "name": "remix-prisma-starter",
  "description": "A Remix starter with Prisma and Vercel",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .eslintcache playwright-report build public/build .cache node_modules",
    "start": "remix-serve build",
    "build": "remix build",
    "dev": "remix dev",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup node",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy && npm run db:generate",
    "db:gen:migration": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "test": "dotenv -e .env.test -- bash -c 'npm run db:reset -- --force && npm run build && playwright test'",
    "test:ci": "npm run db:migrate && npm run build && playwright test",
    "prepare": "husky install",
    "deploy": "npm run ts-run tasks/validateConfig.ts && npm run db:migrate && npm run build",
    "ts-check": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "ts-run": "dotenv ts-node",
    "validate-env": "npm run ts-run tasks/validateConfig.ts",
    "ci": "npm run lint && npm run ts-check && npm run test"
  },
  "dependencies": {
    "@prisma/client": "4.10.1",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "1.13.0",
    "@remix-run/serve": "1.13.0",
    "@remix-run/server-runtime": "^1.13.0",
    "@remix-run/vercel": "1.13.0",
    "accept-language-parser": "^1.5.0",
    "argon2": "^0.30.2",
    "classnames": "^2.3.2",
    "daisyui": "^2.51.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "remix": "1.13.0",
    "zod": "^3.20.6",
    "zustand": "^4.3.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright-testing-library/test": "^4.5.0",
    "@playwright/test": "^1.31.1",
    "@remix-run/dev": "1.13.0",
    "@types/accept-language-parser": "^1.5.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "husky": ">=8.0.2",
    "lint-staged": "^13.1.2",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.4",
    "prisma": "4.10.1",
    "prisma-database-cleaner": "^0.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "zod-error": "^1.5.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix"
  }
}
