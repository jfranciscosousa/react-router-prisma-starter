{
  "private": true,
  "name": "remix-prisma-starter",
  "description": "A Remix starter with Prisma and Vercel",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .eslintcache playwright-report build public/build .cache node_modules",
    "start": "remix-serve build",
    "build": "remix build",
    "dev": "remix dev",
    "postinstall": "remix setup node",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "test": "dotenv -e .env.test -- bash -c 'prisma db push -- --force && npm run build && playwright test'",
    "test:ci": "prisma db push && npm run build && playwright test",
    "prepare": "husky install",
    "deploy": "npm run ts-run tasks/validateConfig.ts && prisma generate && prisma db push && npm run build",
    "ts-check": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "ts-run": "dotenv ts-node",
    "validate-env": "npm run ts-run tasks/validateConfig.ts",
    "ci": "npm run lint && npm run ts-check && npm run test"
  },
  "dependencies": {
    "@prisma/client": "4.16.2",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "1.18.1",
    "@remix-run/serve": "^1.18.1",
    "@remix-run/vercel": "1.18.1",
    "@vercel/remix": "^1.18.0",
    "accept-language-parser": "^1.5.0",
    "argon2": "^0.30.3",
    "classnames": "^2.3.2",
    "daisyui": "^3.1.7",
    "date-fns": "^2.30.0",
    "isbot": "^3.6.12",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "remix": "1.18.1",
    "zod": "^3.21.4",
    "zod-error": "^1.5.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright-testing-library/test": "^4.5.0",
    "@playwright/test": "^1.35.1",
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@types/accept-language-parser": "^1.5.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": ">=8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "prisma": "4.16.2",
    "prisma-database-cleaner": "^0.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=v18.16.1"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix"
  }
}
