{
  "headers": [
    {
      "source": "(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' 'unsafe-inline' gc.zgo.at; img-src 'self' data:;"
        },
        { "key": "X-XSS-Protection", "value": "1" }
      ]
    }
  ]
}
